# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Duffel API Token (Get from https://duffel.com/dashboard)
# For testing, use a test token (starts with duffel_test_)
# For production, use a live token (starts with duffel_live_)
DUFFEL_API_TOKEN=your_duffel_api_token_here

# Server Port
PORT=3000

# Environment Mode
NODE_ENV=development
